<h1>Register Account</h1>
<%= form_for @user do |f| %>
	<%= f.label :screenname %>
	<%= f.text_field :screenname %>
	<%= f.label :password %>
	<%= f.password_field :password %>
	<%= f.label :password_confirmation %>
	<%= f.password_field :password_confirmation %>
	<%= f.submit "Register" %>
<% end %>